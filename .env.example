ADMIN_PASSWORD=ADMIN_PASSWORD

# Optional: point to mysqldump if not in PATH (Windows/cPanel adjust as needed)
# Example (Windows Wamp):
# MYSQLDUMP_PATH=C:\wamp64\bin\mysql\mysql8.0.36\bin\mysqldump.exe
# Example (cPanel typical):
# MYSQLDUMP_PATH=/usr/bin/mysqldump

# Restrict access by IP (comma-separated)
ALLOW_IPS=

# Block cloning on this environment
READ_ONLY=true

# “Diagram” button is disabled when DIAGRAM_ENABLED is false.
DIAGRAM_ENABLED=true